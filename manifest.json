{
    "manifest_version": 3,
    "name": "Gerador de Cupom Fiscal",
    "version": "1.2",
    "description": "Gera e imprime automaticamente um cupom fiscal a partir do HTML da p√°gina.",
    "permissions": ["activeTab", "storage", "scripting"],
    "host_permissions": ["<all_urls>"],
    "options_page": "options.html",
    "action": {
      "default_popup": "popup.html",
      "default_title": "Gerar Cupom"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/printer.png",
      "48": "icons/printer.png",
      "128": "icons/printer.png"
    }
  }
  